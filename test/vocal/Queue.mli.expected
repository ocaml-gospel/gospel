                             {| b = is_empty q
      ensures b <-> q.view = Seq.empty |}]
val transfer : 'a t -> 'a t -> unit[@@gospel
                                     {| transfer q1 q2
      modifies q1.view, q2.view
      ensures  q1.view = Seq.empty
      ensures  q2.view = old q2.view ++ old q1.view |}]

